{
  "name": "plugsafe-landing",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev -- --host",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/**": {
      "memory": 1024,
      "maxDuration": 600
    }
  },
  "git": {
    "deploymentEnabled": true
  }
}
